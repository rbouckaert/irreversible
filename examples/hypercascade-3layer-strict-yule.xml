    <map name="OneOnX" >beast.math.distributions.OneOnX</map>
    
    
              <distribution id="jointTreeLikelihood" spec="irreversible.substmodel.ConditionedOnDistribution">
                 <distrY id="treeLikelihood.Y" spec="TreeLikelihood" data="@hypercascade_2layer" tree="@Tree.t:big2_barcs" useAmbiguities="true">
                    <siteModel id="SiteModel.s:Y" spec="SiteModel" gammaCategoryCount="1" shape="@gammaShape.s:big2_barcs">
                        <mutationRate idref="mutationRate.s:big2_barcs"/>
                        <proportionInvariant idref="proportionInvariant.s:big2_barcs"/>
                        <substModel id="hky.s:Y" spec="irreversible.substmodel.HyperCascadeSubstModel" 
                        	dataType="@HyperCascadeDataType2"
                        	AGRate="@AGRate">
                        </substModel>
                    </siteModel>
                    <branchRateModel idref="StrictClock.c:big2_barcs"/>
                </distrY>

                <distrXY id="treeLikelihood.XY" spec="TreeLikelihood" data="@hypercascade_3layer" tree="@Tree.t:big2_barcs"
                	scaling="always">
                    <siteModel id="SiteModel.s:big2_barcs" spec="SiteModel" gammaCategoryCount="1" shape="@gammaShape.s:big2_barcs">
                        <parameter id="mutationRate.s:big2_barcs" spec="parameter.RealParameter" estimate="false" name="mutationRate">1.0</parameter>
                        <parameter id="proportionInvariant.s:big2_barcs" spec="parameter.RealParameter" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                        <substModel id="hky.s:big2_barcs" spec="irreversible.substmodel.HyperCascadeSubstModel" 
                        	dataType="@HyperCascadeDataType"
                        	AGRate="@AGRate">
                        </substModel>
                    </siteModel>
                    <branchRateModel id="StrictClock.c:big2_barcs" spec="beast.evolution.branchratemodel.StrictClockModel">
                        <parameter id="clockRate.c:big2_barcs" spec="parameter.RealParameter" estimate="false" name="clock.rate">1.0</parameter>
                    </branchRateModel>
                </distrXY>
                
	          </distribution>
    
    
    <run id="mcmc" spec="MCMC" chainLength="1000000" numInitializationAttempts="1000">
        <state id="state" spec="State" storeEvery="5000">
            <tree id="Tree.t:big2_barcs" spec="beast.evolution.tree.Tree" name="stateNode">
                <taxonset id="TaxonSet.big2_barcs" spec="TaxonSet">
                    <alignment idref="hypercascade_hiPSC_barcs_subset"/>
                </taxonset>
            </tree>
            <parameter id="birthRate.t:big2_barcs" spec="parameter.RealParameter" name="stateNode">1.0</parameter>
            <parameter id="AGRate" spec="parameter.RealParameter" dimension="3" lower="0.05" name="stateNode">1.8 1.0 0.2</parameter>
            <parameter id="gammaShape.s:big2_barcs" spec="parameter.RealParameter" name="stateNode" lower="0.1">1.0</parameter>
        </state>

        <init id="RandomTree.t:big2_barcs" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@Tree.t:big2_barcs" taxa="@hypercascade_hiPSC_barcs_subset"
            rootHeight="0.5">
            <populationModel id="ConstantPopulation0.t:big2_barcs" spec="ConstantPopulation">
                <parameter id="randomPopSize.t:big2_barcs" spec="parameter.RealParameter" name="popSize">1</parameter>
            </populationModel>
        </init>


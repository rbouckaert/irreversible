        <distribution id="posterior" spec="util.CompoundDistribution">
            <distribution id="prior" spec="util.CompoundDistribution">
                <distribution id="YuleModel.t:big2_barcs" spec="beast.evolution.speciation.YuleModel" birthDiffRate="@birthRate.t:big2_barcs" tree="@Tree.t:big2_barcs"/>
                <prior id="YuleBirthRatePrior.t:big2_barcs" name="distribution" x="@birthRate.t:big2_barcs">
                    <Gamma id="Gamma.2" name="distr">
                        <parameter id="RealParameter.9" spec="parameter.RealParameter" estimate="false" name="alpha">2.0</parameter>
                        <parameter id="RealParameter.10" spec="parameter.RealParameter" estimate="false" name="beta">1.0</parameter>
                    </Gamma>
                </prior>
                <prior id="GammaShapePrior.s:big2_barcs" name="distribution" x="@gammaShape.s:big2_barcs">
                    <Exponential id="Exponential.0" name="distr">
                        <parameter id="RealParameter.0" spec="parameter.RealParameter" estimate="false" name="mean">1.0</parameter>
                    </Exponential>
                </prior>
                <prior id="AGRate.s:big2_barcs" name="distribution" x="@AGRate">
                    <LogNormal id="LogNormalDistributionModel.1" name="distr">
                        <parameter id="RealParameter.7" spec="parameter.RealParameter" estimate="false" name="M">1.0</parameter>
                        <parameter id="RealParameter.8" spec="parameter.RealParameter" estimate="false" name="S">1.25</parameter>
                    </LogNormal>
                </prior>
            </distribution>
            <distribution id="likelihood" spec="util.CompoundDistribution">
	            <!--
                <distribution idref="treeLikelihood.XY"/>
                -->
                <distribution idref="jointTreeLikelihood"/>
                
            </distribution>
        </distribution>
        
        <operator id="YuleBirthRateScaler.t:big2_barcs" spec="ScaleOperator" parameter="@birthRate.t:big2_barcs" weight="3.0"/>
        <operator id="YuleModelTreeScaler.t:big2_barcs" spec="ScaleOperator" scaleFactor="0.5" tree="@Tree.t:big2_barcs" weight="3.0"/>
        <operator id="YuleModelTreeRootScaler.t:big2_barcs" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5" tree="@Tree.t:big2_barcs" weight="3.0"/>
        <operator id="YuleModelUniformOperator.t:big2_barcs" spec="Uniform" tree="@Tree.t:big2_barcs" weight="30.0"/>
        <operator id="YuleModelSubtreeSlide.t:big2_barcs" spec="SubtreeSlide" tree="@Tree.t:big2_barcs" weight="15.0"/>
        <operator id="YuleModelNarrow.t:big2_barcs" spec="Exchange" tree="@Tree.t:big2_barcs" weight="0.0"/>
        <operator id="YuleModelWide.t:big2_barcs" spec="Exchange" isNarrow="false" tree="@Tree.t:big2_barcs" weight="3.0"/>
        <operator id="YuleModelWilsonBalding.t:big2_barcs" spec="WilsonBalding" tree="@Tree.t:big2_barcs" weight="3.0"/>

        <operator id="gammaShapeScaler.s:big2_barcs" spec="ScaleOperator" parameter="@gammaShape.s:big2_barcs" scaleFactor="0.5" weight="0.1"/>

